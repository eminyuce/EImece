@model List<String>
@{
    var InstagramWebSiteLink = Model[0];
    var TwitterWebSiteLink = Model[1];
    var LinkedinWebSiteLink = Model[2];
    var FacebookWebSiteLink = Model[3];

}

<ul class="social-icons">

    @if (!String.IsNullOrEmpty(FacebookWebSiteLink))
    {
        <li><a class="facebook" href="@FacebookWebSiteLink"><i class="icon-facebook"></i></a></li>
    }
    @if (!String.IsNullOrEmpty(InstagramWebSiteLink))
    {
        <li><a class="instagram" href="@InstagramWebSiteLink"><i class="icon-instagram"></i></a></li>
    }
    @if (!String.IsNullOrEmpty(TwitterWebSiteLink))
    {
        <li><a class="twitter" href="@TwitterWebSiteLink"><i class="icon-twitter"></i></a></li>
    }
    @if (!String.IsNullOrEmpty(LinkedinWebSiteLink))
    {
        <li><a class="linkedin" href="@LinkedinWebSiteLink"><i class="icon-linkedin"></i></a></li>
    }
</ul>