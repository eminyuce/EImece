@using EImece.Domain.Entities;
@using EImece.Domain.Models.FrontModels;
@model MainPageViewModel

@{
    ViewBag.Title = "Home Page";
    var mainPageImages = Model.MainPageImages;
    var mainPageProducts = Model.MainPageProducts;
}


@foreach (var mainPageImage in mainPageImages)
{
    @Html.DisplayFor(i=>mainPageImage)
}
<br />

@foreach (var mainPageProduct in mainPageProducts)
{
    @Html.DisplayFor(i => mainPageProduct,"MainPageProduct")
}
<br />
